<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.zyt.WYSL.WYSLActivity">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="0dp"
        android:layout_marginLeft="0dp"
        android:layout_marginRight="0dp"
        android:background="@color/navigationBar"
        android:id="@+id/relativeLayout11" >

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:text="我要赎楼"
            android:id="@+id/textView66"
            android:textSize="24dp"
            android:textColor="#fff"
            android:layout_centerInParent="true" />

        <RelativeLayout
            android:id="@+id/backBtn"
            android:layout_width="50dp"
            android:layout_height="50dp">
            <ImageView
                android:layout_width="12dp"
                android:layout_height="21dp"
                android:layout_centerVertical="true"
                android:src="@drawable/back"
                android:layout_marginLeft="10dp" />
        </RelativeLayout>

    </RelativeLayout>

    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/relativeLayout11"
        android:layout_marginBottom="60dp"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="10dp"
        android:layout_marginRight="10dp">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:background="#fff"
            android:id="@+id/relativeLayout12" >

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:text="楼盘名称"
                android:id="@+id/textView68"
                android:layout_centerVertical="true"
                android:layout_alignParentStart="true"
                android:layout_marginLeft="10dp"
                android:textSize="16dp"
                android:textColor="#000" />

            <EditText
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:id="@+id/lpmc"
                android:layout_centerVertical="true"
                android:layout_alignParentEnd="true"
                android:layout_toRightOf="@+id/textView68"
                android:background="@null"
                android:layout_marginLeft="100dp"
                android:layout_marginRight="10dp"
                android:textAlignment="viewEnd"
                android:textSize="16dp"
                android:singleLine="true" />
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:background="#fff"
            android:layout_marginTop="1dp"
            android:id="@+id/relativeLayout13" >

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:text="房屋面积"
                android:id="@+id/textView69"
                android:layout_centerVertical="true"
                android:layout_alignParentStart="true"
                android:layout_marginLeft="10dp"
                android:textSize="16dp"
                android:textColor="#000" />

            <EditText
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:id="@+id/fwmj"
                android:layout_centerVertical="true"
                android:layout_alignParentEnd="true"
                android:layout_toRightOf="@+id/textView69"
                android:background="@null"
                android:layout_marginLeft="100dp"
                android:layout_marginRight="35dp"
                android:textAlignment="viewEnd"
                android:inputType="number"
                android:textSize="16dp"
                android:singleLine="true" />

            <RelativeLayout
                android:layout_width="40dp"
                android:layout_height="match_parent"
                android:layout_alignParentRight="true" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceSmall"
                    android:text="m"
                    android:id="@+id/textView116"
                    android:layout_centerInParent="true"
                    android:textSize="16dp" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceSmall"
                    android:text="2"
                    android:id="@+id/textView117"
                    android:layout_alignParentRight="true"
                    android:layout_alignParentTop="true"
                    android:layout_marginRight="8dp"
                    android:layout_marginTop="10dp"
                    android:textSize="12dp" />
            </RelativeLayout>
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:background="#fff"
            android:layout_marginTop="1dp"
            android:id="@+id/relativeLayout14" >

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:text="赎楼金额"
                android:id="@+id/dkjeT"
                android:layout_centerVertical="true"
                android:layout_alignParentStart="true"
                android:layout_marginLeft="10dp"
                android:textSize="16dp"
                android:textColor="#000" />

            <EditText
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:id="@+id/slje"
                android:layout_centerVertical="true"
                android:layout_alignParentEnd="true"
                android:layout_toRightOf="@+id/dkjeT"
                android:background="@null"
                android:layout_marginLeft="100dp"
                android:layout_marginRight="35dp"
                android:textAlignment="viewEnd"
                android:inputType="number"
                android:textSize="16dp"
                android:singleLine="true" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:text="元"
                android:id="@+id/textView118"
                android:layout_centerVertical="true"
                android:layout_alignParentEnd="true"
                android:textSize="16dp"
                android:layout_marginRight="10dp" />
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:background="#fff"
            android:layout_marginTop="1dp"
            android:id="@+id/relativeLayout15" >

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:text="联系人"
                android:id="@+id/lxrT"
                android:layout_centerVertical="true"
                android:layout_alignParentStart="true"
                android:layout_marginLeft="10dp"
                android:textSize="16dp"
                android:textColor="#000" />

            <EditText
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:id="@+id/lxr"
                android:layout_centerVertical="true"
                android:layout_alignParentEnd="true"
                android:layout_toRightOf="@+id/lxrT"
                android:background="@null"
                android:layout_marginLeft="100dp"
                android:layout_marginRight="10dp"
                android:textAlignment="viewEnd"
                android:textSize="16dp"
                android:singleLine="true" />
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:background="#fff"
            android:layout_marginTop="1dp"
            android:id="@+id/relativeLayout16" >

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:text="联系电话"
                android:id="@+id/textView72"
                android:layout_centerVertical="true"
                android:layout_alignParentStart="true"
                android:layout_marginLeft="10dp"
                android:textSize="16dp"
                android:textColor="#000" />

            <EditText
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:id="@+id/lxdh"
                android:layout_centerVertical="true"
                android:layout_alignParentEnd="true"
                android:layout_toRightOf="@+id/textView72"
                android:background="@null"
                android:layout_marginLeft="100dp"
                android:layout_marginRight="10dp"
                android:textAlignment="viewEnd"
                android:inputType="number"
                android:textSize="16dp"
                android:singleLine="true"
                android:hint="联系本机可不填" />
        </RelativeLayout>

    </LinearLayout>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="50dp"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="提交"
        android:id="@+id/submitBtn"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentRight="true"
        android:background="@color/bottomBar"
        android:textColor="#fff"
        android:textSize="18dp"
        android:gravity="center" />
</RelativeLayout>
